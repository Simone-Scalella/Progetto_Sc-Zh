package univpm.OOP2020.body;
/**
 * <p>
 * <b>Classe</b> che contiene componenti di una semplice espressione logica del filtro
 * </p>
 * @author Zhang Yihang & Scalella Simone
 * @see univpm.OOP2020.model.Statics#filter_method(request_body)
 */
public class request_body {
	/**
	 * <code>String</code> che contiene operatore logica (==,<=,>=,<,> ecc) della condizione
	 */
	private String operator = null;
	/**
	 * <code>String</code> che contiene valore in int della condzione
	 */
	private String value = null;
	/**
	 * <code>String</code> che contiene attributo della condizione
	 */
	private String attribute = null;
	
	public request_body() {};
	/**
	 * Restituisce operatore
	 * @return <code>String</code> che contiene operatore logico
	 */
	public String getOperator() {return operator;}
	/**
	 * Restituisce valore
	 * @return
	 */
	public String getValue() {return value;}
	/**
	 * Restituisce attributo
	 * @return
	 */
	public String getAttribute() {return attribute;}	
}
